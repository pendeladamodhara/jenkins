pipeline {
     
       agent any 
          
         stages {
         
	         stage ('compile stage') {
	            withMaven(maven:'3.6.1')
	            {
	             sh 'mvn clean compile'
	            }
	         }
	         stage ('testing  stage') {
	            withMaven(maven:'3.6.1')
	            {
	             sh 'mvn test'
	            }
	         }
           stage ('deploy  stage') {
	            withMaven(maven:'3.6.1')
	            {
	             sh 'mvn deploy'
	            }
	         }
         }
   
}